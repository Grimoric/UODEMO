// UO-C (old)
include spelskil;

scriptvar integer Q463;
scriptvar integer Q5VX;

on creation()
{
  Q5VX = 0x01;
  Q463 = 0x0A;
  return(0x01);
}

on equip(obj equippedon)
{
  Q43H(equippedon, Q5VX, Q463);
  return(0x01);
}

on unequip(obj unequippedfrom)
{
  Q43H(unequippedfrom, Q5VX, 0x00 - Q463);
  return(0x01);
}