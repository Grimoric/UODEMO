// UO-C (old)
include globals;

on speech<"godStart">(obj speaker, string arg)
{
  callback(this, 0x02, 0x24);
  return(0x00);
}

on creation()
{
  callback(this, 0x02, 0x24);
  setType(this, 0x01);
  return(0x00);
}

on callback<0x24>()
{
  list Q5DQ;
  messageToRange(getLocation(this), 0x03, "switchType", Q5DQ);
  callback(this, 0x02, 0x24);
  return(0x00);
}