function integer Q4TL(obj me)
{
  obj multi = getMultiSlaveId(me);
  integer Q4FW = getDecayCount(multi);
  if(getDecayMax(multi) == 0xFFFF)
  {
    resetMultiCarriedDecay(multi);
    return(0x00);
  }
  if(Q4FW > 0x00)
  {
    Q59B(multi);
    resetMultiCarriedDecay(multi);
    return(0x01);
  }
  return(0x00);
}

